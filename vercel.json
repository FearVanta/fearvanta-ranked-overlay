{
    "version": 2,
    "builds": [
        {
            "src": "overlay/**",
            "use": "@vercel/static"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
